{
    "name": "electron-fresh",
    "displayName": "Electron Fresh",
    "description": "Modified electron theme with more visible line highlighting.",
    "version": "1.0.3",
    "publisher": "program247365",
    "license": "MIT",
    "author": {
        "email": "kridgway@gmail.com",
        "name": "Kevin Ridgway",
        "url": "https://kbr.sh"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/program247365/vscode-theme-electron-fresh.git"
    },
    "preview": true,
    "homepage": "https://github.com/program247365/vscode-theme-electron-fresh",
    "icon": "images/icon.png",
    "scripts": {
        "install": "mkdir $HOME/.vscode/extensions/vscode-theme-electron-fresh/ && cp -r ../vscode-theme-electron-fresh/* $HOME/.vscode/extensions/vscode-theme-electron-fresh/",
        "vscode:prepub": "vsce package",
        "pub": "vsce publish $npm_package_version"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Electron Fresh",
                "uiTheme": "vs-dark",
                "path": "./themes/electron-fresh-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.93.0"
    }
}
